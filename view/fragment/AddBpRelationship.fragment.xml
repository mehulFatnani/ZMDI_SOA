	<Dialog id="addRelationDialog" title="Add Relationship">
		<Panel expandable="false" expanded="false" headerText="{AddBpRelationship.header}" width="auto" class="sapUiResponsiveMargin">
			<content>
				<Label text="Main Partner"/>
				<Input id="MainPartnerDesc" editable="false"/>
				<Input id="MainPartner1" editable="false" visible="false"/>
				<Label text="Relationship Category"/>
				<Input id="CatgInput" valueHelpRequest="onCatgValHelp" showValueHelp="true"></Input>
				<Label text="Related Business Partner"/>
				<Input id="BpInput" valueHelpRequest="onBPValHelp" showValueHelp="true"></Input>
				<Button text="Add" press="onSubmitRelationship"/>
				<Button text="Cancel" press="onCloseDialog"/>
			</content>
		</Panel>
