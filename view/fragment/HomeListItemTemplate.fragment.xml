<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:f="sap.ui.layout.form" xmlns:me="sap.me" xmlns:smartField="sap.ui.comp.smartfield" xmlns:smartForm="sap.ui.comp.smartform"
	xmlns="sap.m">
	<ObjectListItem counter="0" markFavorite="false" markFlagged="false" number="{ObjectId}" press="onSelect" showMarkers="true"
		title="{ProcessType}" type="{device&gt;/listItemType}">
		<firstStatus>
			<ObjectStatus icon="sap-icon://bookmark" state="{ path: 'SocAppStatusText', formatter: '._Formatters.formatStatus' }" />
		</firstStatus>
		<secondStatus>
			<ObjectStatus text="{SocAppStatusText}"/>
		</secondStatus>
		<attributes>
			<ObjectAttribute text="{MainPartnerName}"/>
			<ObjectAttribute text="{path:'SocAppDate', formatter: '._Formatters.formatDate'}"/>
		</attributes>
	</ObjectListItem>
</core:FragmentDefinition>